                                    </div>
                                </div>
                            </div>
                            <div class="m-portlet__body">
                                <div class="m-widget3">
                                    @foreach (var trit in Model.TritOthersListModel)
                                    {
                                        <div class="m-widget3__item">
                                            <div class="m-widget3__header">
                                                <div class="m-widget3__user-img">
                                                    <img class="m-widget3__img" src="~/Content/assets/app/media/img/users/user1.jpg" alt="">
